    ICondition condition 	= null;
    IQuery query			= null;
    AggExpr aggExpr			= null;
    RandomSpec randomspec	= null;
}
{
    LOOKAHEAD(7) < DEFINE > lvl = word() { level = Hierarchy.parse(lvl); }
    	property = property() < AS >
